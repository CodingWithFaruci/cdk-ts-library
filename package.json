{
    "name": "cdk-ts-library",
    "version": "0.1.0",
    "bin": {
        "cdk-ts-library": "bin/cdk-ts-library.js"
    },
    "scripts": {
        "lint": "npx eslint **/*.ts --fix --quiet",
        "build": "npx rimraf dist & tsc",
        "build-w": "npx rimraf dist & webpack",
        "watch": "tsc -w",
        "test": "jest --passWithNoTests",
        "diff": "npx cdk diff cdk-ts-library --ci --require-approval never --no-asset-metadata",
        "deploy": "npx cdk deploy cdk-ts-library --ci --require-approval never --no-asset-metadata"
    },
    "devDependencies": {
        "@types/jest": "^28.1.3",
        "@types/node": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^5.29.0",
        "@typescript-eslint/parser": "^5.29.0",
        "aws-cdk": "2.29.1",
        "eslint": "^8.18.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-plugin-jest": "^26.5.3",
        "jest": "^28.1.1",
        "jest-sonar": "^0.2.12",
        "ts-jest": "^28.0.5",
        "ts-loader": "^9.3.1",
        "ts-node": "^10.8.0",
        "typescript": "~4.7.4",
        "webpack": "^5.73.0",
        "webpack-cli": "^4.10.0"
    },
    "dependencies": {
        "aws-cdk-lib": "2.29.1",
        "constructs": "^10.1.42",
        "source-map-support": "^0.5.21"
    }
}
